 int[]arr={2,3,52,524,24,29};

        int i=0;
        int r=arr.length-1;


        while (i<r){
            int temp=arr[i];
            arr[i]=arr[r];
            arr[r]=temp;
            i++;
            r--;

        }

        for(int k=0;k<arr.length;k++){
            System.out.println(arr[k]);
        }
